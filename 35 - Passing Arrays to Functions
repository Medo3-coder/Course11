#include <iostream>

using namespace std;  


void printArray( int theArray[] , int SizeOfArray )
{
    for(int i = 0 ; i < SizeOfArray ;i++)
    cout<< theArray[i]<<endl;
}


int main()
{
    int me[3]= {1 ,2,3};
    int jessy[4] = {140 , 050 ,600 ,879};
    
    printArray(jessy , 4);
    
    
}
